<div class="container">
<h1>All Wikis</h1>

<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
<% end %>
  <div class="col-md-4">
    <%= link_to "NEW WIKI", new_wiki_path, class: 'btn btn-primary' %>    
  </div>
</div>